{
  "name": "game-server-manager",
  "version": "1.0.0",
  "description": "Game Server Manager - Phase 1: Health Monitoring for Unreal Dedicated Servers",
  "main": "game-server-monitor.js",
  "scripts": {
    "start": "node game-server-monitor.js",
    "dev": "nodemon game-server-monitor.js",
    "screen": "screen -S monitor-api -dm node game-server-monitor.js",
    "stop": "screen -S monitor-api -X quit",
    "restart": "npm run stop && sleep 2 && npm run screen",
    "status": "screen -list | grep monitor-api",
    "logs": "screen -r monitor-api"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "game-server",
    "monitoring",
    "unreal-engine",
    "health-check",
    "api",
    "dashboard"
  ],
  "author": "3DDServices Team",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jshell91/3DDServices.git"
  },
  "bugs": {
    "url": "https://github.com/jshell91/3DDServices/issues"
  },
  "homepage": "https://github.com/jshell91/3DDServices#readme"
}