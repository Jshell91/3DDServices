// LINUX SERVER BUILD ON DEVELOPEMENT
cmd.exe /c ""E:/UnrealEngineSC553/UnrealEngine/Engine/Build/BatchFiles/RunUAT.bat"  
-ScriptsForProject="E:/p3DDSW_UE55/p3DDSW_UE55.uproject" Turnkey -command=VerifySdk 
-platform=Linux -UpdateIfNeeded -EditorIO -EditorIOPort=63856  
-project="E:/p3DDSW_UE55/p3DDSW_UE55.uproject" BuildCookRun -nop4 -utf8output -nocompileeditor 
-skipbuildeditor -cook  -project="E:/p3DDSW_UE55/p3DDSW_UE55.uproject" -target=VR3DDSOCIALWORLDServer  
-unrealexe="E:\UnrealEngineSC553\UnrealEngine\Engine\Binaries\Win64\UnrealEditor-Cmd.exe" -platform=Linux 
-stage -archive -package -build -pak -iostore -compressed -prereqs 
-archivedirectory="E:/p3DDSW_UE55/OutPut" -server -noclient -serverconfig=Development" 
-nocompile -nocompileuat

